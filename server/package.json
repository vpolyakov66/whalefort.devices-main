{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "tsoaGen": "npx tsoa spec-and-routes && npx tsc -p ./tsconfig.prod.json",
    "dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec",
    "run:prod": "node ./dist/src/index.js",
    "r:p": "npm run run:prod",
    "db:dump": "docker exec server_database_1 sh -c 'mongodump --archive -u dale -p rootpassword' > ./dump.gz",
    "db:dump:restore": "docker exec -i server-database-1 sh -c \"mongorestore --archive -u dale -p rootpassword\" < ./dump.gz",
    "docker:compose:up": "docker-compose up -d",
    "docker:build:artd": "docker build -t artd/0.0.0 ."
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "bcrypt": "^5.1.0",
    "bcrypt-nodejs": "^0.0.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-sse": "^0.5.3",
    "express-sse-ts": "^1.0.2",
    "firebase": "^9.22.1",
    "firebase-admin": "^11.8.0",
    "helmet": "^6.0.1",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^1.3.1",
    "jet-validator": "^1.0.4",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mongoose": "^7.0.1",
    "morgan": "^1.10.0",
    "rxjs": "^7.8.0",
    "simple-tree-utils": "^1.0.1",
    "sse-pubsub": "^1.4.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "ts-command-line-args": "^2.4.2",
    "tsoa": "^5.1.1",
    "tsyringe": "^4.7.0",
    "jsonpath": "^1.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcrypt-nodejs": "^0.0.31",
    "@types/jsonpath": "^0.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.1",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.0",
    "jasmine": "^4.5.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5"
  }
}
